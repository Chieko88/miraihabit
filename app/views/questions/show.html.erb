<div class="container-fluid">
  <div class="test-card">
    <div class="test-card-content">
      <h2><b>Select your current habits</b></h2>
      <hr>
      <h5><%= @question.content  %></h5>
      <div class="answers-wrapper">
        <% @question.answers.each do |answer|  %>
          <%= render "form", answer: answer %>
        <% end %>
      </div>
    </div>
    <%# <%= render "footer" %>
  </div>
</div>
